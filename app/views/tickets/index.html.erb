<p style="color: green"><%= notice %></p>

<h1>Tickets</h1>

<div id="tickets">
  <% @tickets.each do |ticket| %>
    <%= render ticket %>
    <p>
      <%= link_to "Show this ticket" , project_ticket_path(ticket.project_id,ticket)%>
    </p>
  <% end %>
</div>

<%= link_to "New ticket", new_project_ticket_path %>
